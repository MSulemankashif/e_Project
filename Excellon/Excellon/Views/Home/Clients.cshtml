            // Remove active class from all buttons
            industryBtns.forEach(b => {
                b.classList.remove('border-2', 'border-primary');
                const icon = b.querySelector('div:first-child');
                icon.classList.remove('bg-primary/10');
                icon.classList.add('bg-gray-100');
                const iconInner = icon.querySelector('i');
                iconInner.classList.remove('text-primary');
                iconInner.classList.add('text-gray-600');
            });

            // Add active class to clicked button
            this.classList.add('border-2', 'border-primary');
            const icon = this.querySelector('div:first-child');
            icon.classList.remove('bg-gray-100');
            icon.classList.add('bg-primary/10');
            const iconInner = icon.querySelector('i');
            iconInner.classList.remove('text-gray-600');
            iconInner.classList.add('text-primary');
        });
    });
});
</script>
</body>
</html>