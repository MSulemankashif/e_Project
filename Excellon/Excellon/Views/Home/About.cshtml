        document.addEventListener('DOMContentLoaded', function() {
            // Testimonial slider dots functionality
            const slideDots = document.querySelectorAll('[id^="slide-dot-"]');
            const testimonialSlider = document.querySelector('.testimonial-slider');

            if (slideDots.length > 0 && testimonialSlider) {
                slideDots.forEach((dot, index) => {
                    dot.addEventListener('click', function() {
                        // Reset all dots to inactive state
                        slideDots.forEach(d => d.classList.remove('bg-primary'));
                        slideDots.forEach(d => d.classList.add('bg-gray-300'));

                        // Set clicked dot to active state
                        this.classList.remove('bg-gray-300');
                        this.classList.add('bg-primary');

                        // Calculate scroll position
                        const slideWidth = testimonialSlider.querySelector('div').offsetWidth;
                        testimonialSlider.scrollLeft = slideWidth * index;
                    });
                });
            }
        });

